{
    "query": "WITH sales AS (\n    SELECT p.group1, p.group2, SUM(ol.paid_amount) AS total_sales_amount\n    FROM order_line ol\n    JOIN products p ON ol.product_id = p.product_id\n    WHERE ol.date >= date('2024-06-10', '-28 days')\n    GROUP BY p.group1, p.group2\n),\nlosses AS (\n    SELECT p.group1, p.group2, SUM(l.amount) AS total_loss_amount\n    FROM lost l\n    JOIN products p ON l.product_id = p.product_id\n    WHERE l.date >= date('2024-06-10', '-28 days')\n    GROUP BY p.group1, p.group2\n),\ntotal_losses AS (\n    SELECT SUM(amount) AS total_losses_amount\n    FROM lost\n    WHERE date >= date('2024-06-10', '-28 days')\n)\nSELECT \n    p.group1,\n    p.group2,\n    (COALESCE(s.total_sales_amount, 0)) AS total_sales_amount,\n    (COALESCE(l.total_loss_amount, 0)) AS total_loss_amount,\n    (COALESCE(l.total_loss_amount, 0) / NULLIF(COALESCE(s.total_sales_amount, 1), 0)) * 100 AS pct_loss_from_sales,\n    (COALESCE(l.total_loss_amount, 0) / NULLIF((SELECT total_losses_amount FROM total_losses), 0)) * 100 AS loss_share\nFROM\n    products p\n    LEFT JOIN sales s ON p.group1 = s.group1 AND p.group2 = s.group2\n    LEFT JOIN losses l ON p.group1 = l.group1 AND p.group2 = l.group2\nGROUP BY \n    p.group1, p.group2, s.total_sales_amount, l.total_loss_amount\nORDER BY \n    p.group1, p.group2;\n",
